{
  "name": "@personal/password-manager",
  "version": "1.0.0",
  "description": "Secure, enterprise-grade password management solution",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "test": "jest --coverage",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "dev": "ts-node-dev --respawn src/index.ts",
    "start": "node dist/cjs/index.js",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "ci": "npm run lint && npm run typecheck && npm run test && npm run build"
  },
  "keywords": [
    "password-manager",
    "security",
    "encryption",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "devDependencies": {
    "@types/cypress": "^0.1.6",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.12",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "cypress": "^14.5.1",
    "eslint": "^9.30.1",
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.100.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "crypto-js": "^4.2.0",
    "express": "^5.1.0",
    "pino": "^9.7.0",
    "prom-client": "^15.1.3",
    "zod": "^3.25.76"
  }
}
